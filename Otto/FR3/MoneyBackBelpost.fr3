<TfrxReport Version="4.9.64" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="4095" PreviewOptions.ShowCaptions="True" PreviewOptions.Zoom="1" PreviewOptions.ZoomMode="zmManyPages" PrintOptions.Printer="Default" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="40773,8182756366" ReportOptions.Description.Text="" ReportOptions.LastChange="41520,8741191204" ScriptLanguage="PascalScript" ScriptText.Text="function Floor(const X: Extended): Integer;&#13;&#10;begin&#13;&#10;  Result := Trunc(X);&#13;&#10;  if Frac(X) &#60; 0 then&#13;&#10;    Dec(Result);&#13;&#10;end;&#13;&#10;  &#13;&#10;function RoundPrecision(aValue, aPrecision: Extended):Extended;&#13;&#10;begin&#13;&#10;  Result:= Floor(aValue/aPrecision + 5/9) * aPrecision&#13;&#10;end;&#13;&#10;  &#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;const&#13;&#10;  aPrecision = 50;                                           &#13;&#10;begin&#13;&#10;  if &#60;qryMoneyBacks.&#34;AMOUNT_BYR&#34;&#62; &#62; 5000000 then&#13;&#10;    Set('fee', '0.005')&#13;&#10;  else              &#13;&#10;  if &#60;qryMoneyBacks.&#34;AMOUNT_BYR&#34;&#62; &#62; 3000000 then&#13;&#10;    Set('fee', '0.01')&#13;&#10;  else              &#13;&#10;  if &#60;qryMoneyBacks.&#34;AMOUNT_BYR&#34;&#62; &#62; 1000000 then&#13;&#10;    Set('fee', '0.02')&#13;&#10;  else              &#13;&#10;  if &#60;qryMoneyBacks.&#34;AMOUNT_BYR&#34;&#62; &#62; 100000 then&#13;&#10;    Set('fee', '0.03')&#13;&#10;  else              &#13;&#10;    Set('fee', '0.05');  &#13;&#10;&#13;&#10;  if &#60;qryMoneyBacks.&#34;PAYBYFIRM&#34;&#62; = 1 then&#13;&#10;  begin              &#13;&#10;    set('payed_byr', RoundPrecision(&#60;qryMoneyBacks.&#34;AMOUNT_BYR&#34;&#62;, aPrecision));  &#13;&#10;    set('post_fee_byr', 0);&#13;&#10;    set('fee_by_firm', round(&#60;qryMoneyBacks.&#34;AMOUNT_BYR&#34;&#62;*&#60;fee&#62;));&#13;&#10;    set('nds_byr', round(&#60;fee_by_firm&#62;*0.2));&#13;&#10;  end&#13;&#10;  else&#13;&#10;  begin              &#13;&#10;    set('payed_byr',  RoundPrecision(&#60;qryMoneyBacks.&#34;AMOUNT_BYR&#34;&#62;/(1+&#60;fee&#62;), aPrecision));&#13;&#10;    set('post_fee_byr', RoundPrecision(&#60;qryMoneyBacks.&#34;AMOUNT_BYR&#34;&#62;*&#60;fee&#62;/(1+&#60;fee&#62;), aPrecision));&#13;&#10;    set('fee_by_firm', 0);                                                             &#13;&#10;    set('nds_byr', 0);&#13;&#10;  end;                  &#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;end." StoreInDFM="False" PropData="">
