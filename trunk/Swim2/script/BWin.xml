    <!-- Убираем NOBR -->
    <DeleteAll Text="&lt;nobr&gt;" />
    <DeleteAll Text="&lt;/nobr&gt;" />
    <!-- Убираем картинки -->
    <DeleteAll Start="&lt;img " End="&gt;" />
    <!-- Убираем ячейку с дополнительными событиями -->
    <DeleteAll Start="&lt;td class=" Contain1="rightcell" End="&lt;/td&gt;" />
    <!-- Убираем TBODY -->
    <DeleteAll Start="&lt;tbody" End="&gt;" />
    <DeleteAll Text="&lt;/tbody&gt;" />
