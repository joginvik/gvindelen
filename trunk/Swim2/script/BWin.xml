    <ReplaceAll Text="&amp;nbsp;" New=" " />
    <ReplaceAll Text="&#9;" New=" " />
    <ReplaceAll Text="&CR;" New=" " />
    <ReplaceAll Text="&LF;" New=" " />
    <ReplaceAll Text="-.--" New=" " />
    <ReplaceAll Text="    " New=" " />
    <ReplaceAll Text="   " New=" " />
    <ReplaceAll Text="  " New=" " />
    <ReplaceAll Text="  " New=" " />
    <ReplaceAll Text=" &lt;" New="&lt;" />
    <ReplaceAll Text="&gt; " New="&gt;" />
    <!-- Убираем события -->
    <DeleteAllAttribute Name="onmouseover" />
    <DeleteAllAttribute Name="onmouseout" />
    <DeleteAllAttribute Name="onclick" />
    <DeleteAllAttribute Name="bgcolor" />
    <DeleteAllAttribute Name="class" Value="unselected"/>
    <!-- Убираем заголовок -->
