  <Event Name="BeforeSave">
    <ExtractTags Start="&lt;table class=ot " End="&lt;/table&gt;" />
    <!-- Убираем пробелы -->
    <ReplaceAll Text="&amp;nbsp;" New=" " />
    <ReplaceAll Text="&CR;" New=" " />
    <ReplaceAll Text="&LF;" New=" " />
    <ReplaceAll Text="&gt;--&lt;" New="&gt;&lt;" />
    <ReplaceAll Text="   " New=" " />
    <ReplaceAll Text="  " New=" " />
    <ReplaceAll Text=" &lt;" New="&lt;" />
    <!-- Убираем скрипты -->
    <DeleteAll Start="&lt;script " End="&lt;/script&gt;" />
    <!-- Убираем форму -->
    <DeleteAll Start="&lt;form " End="&gt;" />
    <DeleteAll Text="&lt;/form&gt;" />
    <!-- Убираем инпуты -->
    <DeleteAll Start="&lt;input " End="&gt;" />
    <DeleteAll Text="&lt;/Input&gt;" />
    <!-- Убираем ссылки -->
    <DeleteAll Start="&lt;a " End="&gt;" />
    <DeleteAll Text="&lt;/a&gt;" />
    <!-- Убираем картинки -->
    <DeleteAll Start="&lt;img " End="&gt;" />
    <!-- Убираем события -->
    <DeleteAllAttribute Name="onmouseover" />
    <DeleteAllAttribute Name="onmouseout" />
    <DeleteAllAttribute Name="onclick" />
    <DeleteAllAttribute Name="bgcolor" />
    <!-- Убираем NOBR -->
    <DeleteAll Text="&lt;nobr&gt;" />
    <DeleteAll Text="&lt;/nobr&gt;" />
    <!-- Убираетм TBODY -->
    <DeleteAll Start="&lt;tbody" End="&gt;" />
    <DeleteAll Text="&lt;/tbody&gt;" />
    <!-- Убираем DIV -->
    <DeleteAll Text="&lt;/div&gt;" />
    <DeleteAll Start="&lt;div " End="&gt;" />
    <!-- Убираем коментарии -->
    <DeleteAll Start="&lt;!--" End="--&gt;" />
    <!-- Патчим глюк конторы -->
    <ReplaceAll Text="VFM - Volleyball Franches-Montagnes" New="VFM Volleyball Franches-Montagnes" />
  </Event>

