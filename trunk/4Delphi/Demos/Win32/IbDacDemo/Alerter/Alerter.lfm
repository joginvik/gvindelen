  object scCreate: TIBCScript
    SQL.Strings = (
      'SET TERM ^;'
      ''
      'CREATE PROCEDURE PR_IBC_DEPT_CHANGE(PR_EVENT VARCHAR(64)) '
      'AS '
      'BEGIN '
      '  POST_EVENT PR_EVENT; '
      'END^'
      ''
      'CREATE TRIGGER TR_IBC_DEPT_INS FOR DEPT'
      'AFTER INSERT'
      'AS '
      'BEGIN '
      '  POST_EVENT &TR_EVENT;'
      'END^'
      ''
      'CREATE TRIGGER TR_IBC_DEPT_DEL FOR DEPT'
      'AFTER DELETE'
      'AS '
      'BEGIN '
      '  POST_EVENT &TR_EVENT;'
      'END^'
      ''
      'CREATE TRIGGER TR_IBC_DEPT_UPD FOR DEPT'
      'AFTER UPDATE'
      'AS '
      'BEGIN '
      '  POST_EVENT &TR_EVENT;'
      'END^'
      ''
      'SET TERM ;^'
    )
    left = 240
    top = 160
    MacroData = <    
      item
        Name = 'TR_EVENT'
      end>
  end
