CREATE TABLE DOCUMENTS (
    DOCUMENT_ID  ID_OBJECT NOT NULL /* ID_OBJECT = INTEGER */,
    REFDOC_ID    ID_OBJECT /* ID_OBJECT = INTEGER */
);
ALTER TABLE DOCUMENTS ADD CONSTRAINT PK_DOCUMENTS PRIMARY KEY (DOCUMENT_ID);
ALTER TABLE DOCUMENTS ADD CONSTRAINT FK_DOCUMENT2DOCUMENT FOREIGN KEY (REFDOC_ID) REFERENCES DOCUMENTS (DOCUMENT_ID) ON UPDATE CASCADE;

ALTER TABLE DOCUMENTS ADD CONSTRAINT FK_DOCUMENT2OBJECT FOREIGN KEY (DOCUMENT_ID) REFERENCES OBJECTS (OBJ_ID) ON DELETE CASCADE ON UPDATE CASCADE;
