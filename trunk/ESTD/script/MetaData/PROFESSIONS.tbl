CREATE TABLE PROFESSIONS (
    PROF_ID        ID_OBJECT NOT NULL /* ID_OBJECT = INTEGER */,
    PROF_CODE      CODE_PROF NOT NULL /* CODE_PROF = NUMERIC(5,0) */,
    PROF_NAME      NAME_OBJECT /* NAME_OBJECT = VARCHAR(255) */,
    PROF_OKR_CODE  CODE_PROFOKR /* CODE_PROFOKR = NUMERIC(5,0) */
);
ALTER TABLE PROFESSIONS ADD CONSTRAINT PK_PROFESSIONS PRIMARY KEY (PROF_ID);
ALTER TABLE PROFESSIONS ADD CONSTRAINT FK_PROFESSI_PROF2OKR_PROFS_OK FOREIGN KEY (PROF_OKR_CODE) REFERENCES PROFS_OKR (PROF_OKR_CODE);

CREATE UNIQUE INDEX UK_PROFESSIONS ON PROFESSIONS (PROF_CODE);