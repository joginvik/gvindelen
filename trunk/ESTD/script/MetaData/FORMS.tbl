CREATE TABLE FORMS (
    FORM_ID      ID_FORM NOT NULL /* ID_FORM = INTEGER */,
    DOCUMENT_ID  ID_OBJECT NOT NULL /* ID_OBJECT = INTEGER */,
    PAGE_NO      NUM_POSITION NOT NULL /* NUM_POSITION = INTEGER */,
    FORM_SIZE    NUM_POSITION NOT NULL /* NUM_POSITION = INTEGER */,
    PAGE_LEN     NUM_POSITION DEFAULT 0 NOT NULL /* NUM_POSITION = INTEGER */
);
ALTER TABLE FORMS ADD CONSTRAINT PK_FORMS PRIMARY KEY (FORM_ID);
ALTER TABLE FORMS ADD CONSTRAINT FK_FORMS_DOCUMENT FOREIGN KEY (DOCUMENT_ID) REFERENCES DOCUMENTS (DOCUMENT_ID) ON DELETE CASCADE ON UPDATE CASCADE;
