  object IBCQuery: TIBCQuery
    SQLInsert.Strings = (
      'INSERT INTO EMP'
      '  (EMPNO, ENAME, JOB, MGR, HIREDATE, SAL, COMM, DEPTNO)'
      'VALUES'
      '  (:1, :2, :3, :4, :5, :6, :7, :8)'
    )
    SQLDelete.Strings = (
      'DELETE FROM EMP'
      'WHERE'
      '  EMPNO = :Old_1'
    )
    SQLUpdate.Strings = (
      'UPDATE EMP'
      'SET'
      '  EMPNO = :1, ENAME = :2, JOB = :3, MGR = :4, HIREDATE = :5, SAL = :6, COMM = :7, DEPTNO = :8'
      'WHERE'
      '  EMPNO = :Old_1'
    )
    SQLRefresh.Strings = (
      'SELECT EMP.EMPNO AS IBC$0, EMP.ENAME AS IBC$1, EMP.JOB AS IBC$2, EMP.MGR AS IBC$3, EMP.HIREDATE AS IBC$4, EMP.SAL AS IBC$5, EMP.COMM AS IBC$6, EMP.DEPTNO AS IBC$7 FROM EMP'
      'WHERE EMP.EMPNO = :Old_1 '
    )
    SQL.Strings = (
      'SELECT Emp.*'
      'FROM'
      '  Emp'
    )
    AfterExecute = IBCQueryAfterExecute
    left = 14
    top = 34
  end
